{
    "manifest_version": 3,
    "name": "Melotune",
    "version": "1.0",
    "description": "A tool to find the track details and audio features.",
    "action": {
      "default_popup": "popup.html"
    },
    "permissions": [
      "tabs", "webNavigation", "activeTab", "storage", "identity",
      "declarativeContent"
    ],
    "background": {
      "service_worker": "background.js"
    }
  }
  